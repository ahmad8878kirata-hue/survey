{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server.js"
        },
        {
            "source": "/send-email",
            "destination": "/server.js"
        },
        {
            "source": "/((?!api|send-email).*)",
            "destination": "/server.js"
        }
    ]
}